{
    "/api/v2/copilot/model/{copilot_model_id}": {
        "get": {
            "summary": "Get a Copilot model.",
            "operationId": "GetCopilotModel",
            "responses": {
                "200": {
                    "description": "A successful response.",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "ID of the model. Must be unique."
                            },
                            "provider": {
                                "type": "string",
                                "description": "The Model Provider. E.g. 'Amazon Bedrock' or 'CML Serving'."
                            },
                            "name": {
                                "type": "string",
                                "description": "The name of the model. E.g. 'Llama2 70b'.  The combination of the\nprovider and name fields must be unique."
                            },
                            "endpoint": {
                                "type": "string",
                                "description": "For CMLServing models, the model endpoint string to connect to."
                            },
                            "enabled": {
                                "type": "boolean",
                                "format": "boolean",
                                "description": "Whether a model is enabled for Copilot use."
                            },
                            "default": {
                                "type": "boolean",
                                "format": "boolean",
                                "description": "Whether this model is the default model for Copilot use. Only one model\ncan be the default Copilot model. Only enabled models can be the default."
                            }
                        },
                        "description": "Models for use with Cloudera Copilot."
                    }
                },
                "default": {
                    "description": "An unexpected error response",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "error": {
                                "type": "string"
                            },
                            "code": {
                                "type": "integer",
                                "format": "int32"
                            },
                            "message": {
                                "type": "string"
                            },
                            "details": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "type_url": {
                                            "type": "string"
                                        },
                                        "value": {
                                            "type": "string",
                                            "format": "byte"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "copilot_model_id",
                    "description": "The Copilot model's ID.",
                    "in": "path",
                    "required": true,
                    "type": "string"
                }
            ],
            "tags": [
                "CMLService"
            ]
        }
    },
